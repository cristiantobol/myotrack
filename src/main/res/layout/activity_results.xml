<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Results"
    tools:layout_editor_absoluteY="89dp">

    <ImageView
        android:id="@+id/resultsBodyImg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/man" />

    <TextView
        android:id="@+id/rightBicepsTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="163dp"
        android:gravity="left"
        android:text="Right Biceps:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/leftBicepsTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="89dp"
        android:layout_marginRight="89dp"
        android:layout_marginTop="158dp"
        android:gravity="right"
        android:text="Left Biceps:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/rightBicepsDetailsText"
        android:layout_width="26dp"
        android:layout_height="47dp"
        android:layout_marginTop="11dp"
        android:gravity="left"
        android:text="90%"
        app:layout_constraintStart_toStartOf="@+id/rightBicepsTextView"
        app:layout_constraintTop_toBottomOf="@+id/rightBicepsTextView" />

    <TextView
        android:id="@+id/leftBicepsDetailsText"
        android:layout_width="36dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:gravity="right"
        android:text="89%"
        app:layout_constraintStart_toStartOf="@+id/leftBicepsTextView"
        app:layout_constraintTop_toBottomOf="@+id/leftBicepsTextView" />

    <TextView
        android:id="@+id/LevelOfEngagementTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Level of Engagement"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>